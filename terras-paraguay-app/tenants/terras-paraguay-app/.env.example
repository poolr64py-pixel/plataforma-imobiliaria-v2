# Configuração do tenant Terras Paraguay

# Backend API
REACT_APP_API_URL=http://localhost:3001
REACT_APP_API_TENANT=terras-paraguay

# Tenant específico
REACT_APP_TENANT_SLUG=terras-paraguay
REACT_APP_TENANT_NAME="Terras no Paraguay"

# Google Maps
REACT_APP_GOOGLE_MAPS_KEY=your_google_maps_key

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=your_analytics_id
REACT_APP_FACEBOOK_PIXEL_ID=your_pixel_id

# WhatsApp Business
REACT_APP_WHATSAPP_NUMBER=+595981123456

# Configurações específicas Paraguay
REACT_APP_PRIMARY_CURRENCY=USD
REACT_APP_SECONDARY_CURRENCY=PYG
REACT_APP_TERTIARY_CURRENCY=BRL
REACT_APP_DEFAULT_LANGUAGE=pt
REACT_APP_SUPPORTED_LANGUAGES=pt,es

# Branding
REACT_APP_PRIMARY_COLOR=#22c55e
REACT_APP_SECONDARY_COLOR=#15803d
REACT_APP_ACCENT_COLOR=#fbbf24

# Features habilitadas
REACT_APP_ENABLE_VIRTUAL_TOURS=true
REACT_APP_ENABLE_CRM=true
REACT_APP_ENABLE_MULTI_CURRENCY=true
REACT_APP_ENABLE_MULTI_LANGUAGE=true

# Sentry (monitoramento)
REACT_APP_SENTRY_DSN=your_sentry_dsn

# Build específico
GENERATE_SOURCEMAP=false
BUILD_PATH=../../../dist/terras-paraguay
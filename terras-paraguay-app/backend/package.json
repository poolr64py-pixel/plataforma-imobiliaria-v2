{
  "name": "imobiliario-whitelabel-backend",
  "version": "1.0.0",
  "description": "Backend multi-tenant para plataforma imobili√°ria white-label",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "init": "node scripts/init-database.js",
    "setup": "npm install && npm run init",
    "reset": "npm run init",
    "test:tenant": "node scripts/test-multi-tenant.js",
    "build": "tsc",
    "docker:build": "docker build -t whitelabel-backend .",
    "docker:run": "docker run -p 3001:3001 whitelabel-backend"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "sequelize": "^6.32.1",
    "redis": "^4.6.8",
    "multer": "^1.4.5-lts.1",
    "cloudinary": "^1.40.0",
    "nodemailer": "^6.9.4",
    "stripe": "^13.5.0",
    "morgan": "^1.10.0",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.48.0",
    "sequelize-cli": "^6.6.1",
    "@types/node": "^20.5.9",
    "typescript": "^5.2.2"
  },
  "keywords": ["white-label", "real-estate", "multi-tenant", "api"],
  "author": "Seu Nome",
  "license": "MIT"
}